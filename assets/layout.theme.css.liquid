/* Resources Page */
.template-page-resources .container {

}

.resources-hero {
	height: 150px;
}

.resources-hero .container {
	position: relative;
	height: 100%;
}

.resources-hero .resources-hero__title {
	position: absolute;
	bottom: 0;
}

.resources-main {
	padding: 50px 0;
}

@media screen and (max-width: 767px) {
	.resources-main {
		padding: 30px 0;
	}
}

@media screen and (max-width: 767px) {
	.resources-main__right {
		margin-top: 10px;
	}
}

.resources-main .product-detail-sheets h2 {
	margin-bottom: 30px;
}

.resources-main .product-detail-sheet a {
	font-size: 29px;
    color: var(--color-light-green);
    display: inline-flex;
    margin-bottom: 40px;
    line-height: 29px;
    border-bottom: 1px var(--color-light-green) solid; 
    align-items: center;
    font-weight: 500;
}

.resources-main .product-detail-sheet:last-child a {
	margin-bottom: 0;
}

@media screen and (max-width: 767px) {
	.resources-main .product-detail-sheets a {
		font-size: 20px;
		margin-bottom: 20px;
	}
}

.resources-main .product-detail-sheets .product-detail-sheet svg {
	width: 22px;
	margin-left: 10px;
}

.resources-main .product-detail-sheets .product-detail-sheet svg path{
	fill: var(--color-light-green);
}

.resources-main .ceus-accordions {
	margin-top: 60px;
}

.resources-main .ceus-accordions .accordion-container {
	line-height: 50px;
	border-top: #d5d5d5 solid 1px;
	padding: 12px 0;
}


.resources-main .ceus-accordions .accordion-container:last-child {
	border-bottom: #d5d5d5 solid 1px;
}

.resources-main .ceus-accordions .accordion-container h3 {
	margin: 0;
	text-align: left;
	width: calc(100% - 57px);
}

.resources-main .ceus-accordion--title {
	display: flex;
	cursor: pointer;
}

.ceus-reveal--icon-container {
    width: 40px;
    height: 40px;
    background: #f2f2f2;
    border-radius: 50%;
    position: relative;
    margin-right: 12px;
}

.ceus-reveal--icon-container .ceus-reveal--icon {
	width: 14px;
    margin: 6px auto 0;
}

.ceus-accordion__panel {
	padding-left: 57px;
	display: none;
}

.ceus-accordion__panel a {
	color: var(--color-light-green);
	text-decoration: underline;
}

.about-our-presenters {
	padding: 30px;
	background: var(--color-light-grey);
}

.about-our-presenters h2 {
	margin-bottom: 40px;	
}

@media screen and (max-width: 1024px) {
	.about-our-presenters h2 {
		margin-bottom: 30px;
	}
}

@media screen and (max-width: 1024px) {
	.about-our-presenters {
		padding: 15px;
	}
}

.about-our-presenters .presenters-list {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}

.about-our-presenters .presenters-list .presenters-list__item {
	width: calc(50% - 12px);
	padding-right: 30px;
	margin-bottom: 30px;
}

@media screen and (max-width: 1024px) {
	.about-our-presenters .presenters-list .presenters-list__item {
		padding-right: 0px;
	}
}

@media screen and (max-width: 480px) {
	.about-our-presenters .presenters-list .presenters-list__item {
		width: 100%;
	}
}


.presenters-list__item .presenter-img {
	display: block;
}

.presenters-list__item .presenter-img img {
	cursor: pointer;
}

.presenters-list__item .presenter-name {
	margin: 20px 0;
}

.presenters-list__item .presenter-role {
	margin-top: 0;
}

.presenters-list__item .presenter-read-more,
.presenter-lightbox .presenter-read-more {
	color: var(--color-light-green);
	border-bottom: 1px var(--color-light-green) solid;
	display: inline-block;
	line-height: 1rem;
}

/*resources presenter light box */
.template-page-resources .blocker {
	z-index: 50;
	background-color: rgba(75, 152, 175, 0.75);
    box-shadow: none;
    border-radius: 0;
}

.template-page-resources .presenter-lightbox {
	max-width: 1300px;
	padding: 50px;
	box-shadow: none;
	border-radius: 0;
}

.template-page-resources .presenter-lightbox .presenter-role {
	margin-bottom: 30px;
}

.template-page-resources .presenter-lightbox .presenter-description {
	font-size: 20px;
}

@media screen and (max-width: 1199px) {
	.template-page-resources .presenter-lightbox {
		padding: 30px;
	}

	.template-page-resources .presenter-lightbox .presenter-description {
		font-size: 17px;
	}
}

@media screen and (max-width: 767px) {
	.template-page-resources .presenter-lightbox {
		padding: 15px;
	}
}

.presenter-lightbox--inner {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}

.modal.presenter-lightbox a.close-modal {
	top: 0;
	right: 0;
	background-image: url(lightbox-close.png);
	background-size: cover;
	width: 40px;
	height: 40px;
}

.presenter-lightbox .presenter-lightbox--image {
	display: flex;
	align-items: center;
}

.presenter-lightbox .presenter-lightbox--info {
	padding-left: 40px;
}

.presenter-lightbox .presenter-lightbox--info .presenter-role {
	margin-top: 0;
}

@media screen and (max-width: 1199px) {
	.presenter-lightbox .presenter-lightbox--info {
		padding-left: 20px;
	}
}

@media screen and (max-width: 767px) {
	.presenter-lightbox .presenter-lightbox--info {
		padding-left: 0;
	}
}



